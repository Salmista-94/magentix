	<processing>


	
	    <job name="Install MySql service: x86_64 architecture" condition="!is32bit">
	        <os family="unix" />
	        <executeForPack name="MySql"/>
	        <executefile name="$INSTALL_PATH/bin/MySQL/install-mysql-service-x86_64.sh">
			<env>MYSQL_HOME=$INSTALL_PATH</env>
		</executefile>
	    </job> 

	

	    <job name="Install Tomcat Service">
	        <executeForPack name="Tomcat7"/>
	        <os family="unix" />
		
	        <executefile name="$INSTALL_PATH/bin/Tomcat/install-catalina.sh">
			<env>CATALINA_HOME=$INSTALL_PATH/apache-tomcat-7.0.4</env>
			<env>CATALINA_BASE=$INSTALL_PATH/apache-tomcat-7.0.4</env>
			<env>CATALINA_TMPDIR=$INSTALL_PATH/apache-tomcat-7.0.4/temp</env>
			<env>THOMAS_PATH=$INSTALL_PATH/thomas</env>
			<!--<arg>DIRECTORIO</arg><arg>$INSTALL_PATH</arg>-->
		</executefile>	
	
	    </job>
	
