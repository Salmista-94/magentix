<assembly>
	<id>jar-with-dependencies</id>
	<baseDirectory>/</baseDirectory>
	<moduleSets>
		<moduleSet>
			<includes>
				<include>org.apache.axis2:addressing</include>
			</includes>
		</moduleSet>
	</moduleSets>
	<dependencySets>
		<dependencySet>
			<unpack>true</unpack>
			<outputDirectory></outputDirectory>
		</dependencySet>
	</dependencySets>
	<formats>
		<!-- Formatos en el que queremos el zip -->
		<format>jar</format>
	</formats>
</assembly>
